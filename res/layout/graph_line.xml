<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rl_graph_root"
    android:layout_width="332dp"
    android:layout_height="236dp"
    android:layout_marginTop="8dp"
    android:background="#ffffff" >

    <TextView
        android:id="@+id/tv_date"
        android:layout_width="152dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="8dp"
        android:text="08월 28일"
        android:textColor="#0065a2"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/tv_unit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/tv_date"
        android:layout_below="@id/tv_date"
        android:layout_marginTop="2dp"
        android:gravity="center_vertical"
        android:text="mg/dL"
        android:textColor="#9e9e9e"
        android:textSize="12sp" />

    <ImageView
        android:id="@+id/iv_legend_first"
        android:layout_width="26dp"
        android:layout_height="10dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="39dp"
        android:layout_marginTop="14dp" />

    <TextView
        android:id="@+id/tv_legend_first"
        android:layout_width="31dp"
        android:layout_height="16dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="8dp"
        android:layout_marginTop="11dp"
        android:gravity="center"
        android:textColor="#383838"
        android:textSize="12sp" />

    <ImageView
        android:id="@+id/iv_legend_second"
        android:layout_width="26dp"
        android:layout_height="10dp"
        android:layout_alignLeft="@id/iv_legend_first"
        android:layout_below="@id/iv_legend_first"
        android:layout_marginTop="6dp" />

    <TextView
        android:id="@+id/tv_legend_second"
        android:layout_width="31dp"
        android:layout_height="16dp"
        android:layout_alignLeft="@id/tv_legend_first"
        android:layout_below="@id/tv_legend_first"
        android:gravity="center"
        android:textColor="#383838"
        android:textSize="12sp" />

    <ImageView
        android:id="@+id/iv_range_first"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="50dp"
        android:background="#eaf4f8"
        android:visibility="invisible" />

    <ImageView
        android:id="@+id/iv_range_second"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="50dp"
        android:background="#fef5e0"
        android:visibility="invisible" />

    <com.echo.holographlibrary.LineGraph
        android:id="@+id/linegraph"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="50dp" />

    <RelativeLayout
        android:id="@+id/rl_goal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="56dp"
        android:orientation="vertical"
        android:visibility="invisible" >

        <TextView
            android:id="@+id/tv_goal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:textColor="#78adcc"
            android:textSize="10sp" />

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:background="#78adcc" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_tooltip"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="50dp" />

    <RelativeLayout
        android:id="@+id/rl_graph_x_label_day"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/rl_tooltip"
        android:layout_marginLeft="22dp"
        android:layout_marginRight="15dp"
        android:layout_marginTop="5dp"
        android:visibility="invisible" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="00:00"
            android:textColor="#424242"
            android:textSize="10sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="12:00"
            android:textColor="#424242"
            android:textSize="10sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:text="24:00"
            android:textColor="#424242"
            android:textSize="10sp" />
    </RelativeLayout>

    <LinearLayout
